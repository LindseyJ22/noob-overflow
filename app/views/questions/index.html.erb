<h1>Questions#index</h1>
<p>Find me in app/views/questions/index.html.erb</p>

<div class="questions">

	<% @questions.each do |question| %>
		
		<%= render :partial => "question", :locals => { :question => question } %>
		<a href="#"><%= link_to "View Full Question...", question %></a>

	<% end %>

</div>
