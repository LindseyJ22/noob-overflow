if ("<%= @reputation.rep_given %>" === 'true') {
	let true_start = parseInt($("#rep_value<%= @reputatable.id %>").text());
	$('#rep_value<%= @reputatable.id %>').text(5 + true_start);
}else{
	let false_start = parseInt($("#rep_value<%= @reputatable.id %>").text());
	$('#rep_value<%= @reputatable.id %>').text(false_start - 5);
};
